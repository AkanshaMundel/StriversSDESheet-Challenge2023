#include <bits/stdc++.h> 
void sort012(int *arr, int n)
{
   //   Write your code here
//  Dutch Nationaint l flag algorithm. -->optiminal apprch;
int h= n-1;
int mid=0;
int l =0;
while(mid<=h){
   if(arr[mid]==0){
      swap(arr[l],arr[mid]);
      mid++; l++;
   }
  else if(arr[mid]==1){
      mid++;
   }
   else{
       swap(arr[h], arr[mid]);
       h--;
       
   }
}

}
